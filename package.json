{
  "name": "luxon",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Immutable date wrapper",
  "author": "Isaac Cambron",
  "keywords": [
    "date",
    "immutable"
  ],
  "repository": "https://github.com/moment/luxon",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "docs": "npx tsc -t es6 --outDir transpiled && esdoc -c docs/index.js",
    "site": "cp -r site dist",
    "benchmark": "echo todo benchmarks/datetime.ts",
    "codecov": "codecov",
    "check-doc-coverage": "grep '\"coverage\": \"100%\"' dist/docs/coverage.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/luxon.esm.js",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "codecov": "^3.6.5",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  }
}
